read -a array
for i in "${array[*]}"
do
        echo "$i"
done
len=${#array[@]}
echo $len
for (( i=0;i< $len;i++ ))
do
        for(( j=0;j< $(( $len - i - 1));j++ ))
        do
                if [ ${array[j]} -gt ${array[$(( j + 1))]} ]
                then
                            temp=${array[j]}
                        array[j]=${array[$(( j + 1))]}
                         array[$(( j + 1))]=$temp
                fi
        done
done
echo ${array[@]}
